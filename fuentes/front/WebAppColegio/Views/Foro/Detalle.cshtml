                                                    @{
                                                        if (item.sexo == "M")
                                                        {
                                                            <img class="img-profile rounded-circle" src="/masterpage/img/man.png" alt=item.creador >
                                                        }
                                                        else
                                                        {
                                                            <img class="img-profile rounded-circle" src="/masterpage/img/girl.png" alt=item.creador >
                                                        }
                                                    } 
