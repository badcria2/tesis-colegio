                        <a href="" class="ipsUserPhoto ipsUserPhoto_small">
                            @{
                                if (Model.FirstOrDefault().sexo == "M")
                                {
                                    <img class="img-profile rounded-circle" src="/masterpage/img/man.png" alt=@Model.FirstOrDefault().creador >
                                }
                                else
